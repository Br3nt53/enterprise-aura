{
  "$schema": "https://raw.githubusercontent.com/microsoft/pyright/master/packages/pyright/config.schema.json",
  "venvPath": "/workspaces/enterprise-aura",
  "venv": ".venv",
  "include": [
    "aura_v2",
    "tests"
  ],
  "exclude": [
    "**/.venv",
    "**/.git",
    "**/__pycache__"
  ],
  "executionEnvironments": [
    {
      "root": ".",
      "pythonVersion": "3.11",
      "extraPaths": [
        "aura_v2",
        "scripts"
      ]
    }
  ],
  "reportMissingImports": "warning",
  "reportMissingTypeStubs": "none"
}
